{
    "name": "dgtlss/synapse",
    "description": "Synapse: Seamlessly integrate AI capabilities into your Laravel Eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "eloquent", "ai", "vector", "embeddings", "semantic-search"],
    "authors": [
        {
            "name": "Nathan Langer",
            "email": "nathanlanger@googlemail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|12.0",
        "illuminate/database": "^11.0|12.0",
        "illuminate/container": "^11.0|12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|11.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Dgtlss\\Synapse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgtlss\\Synapse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dgtlss\\Synapse\\SynapseServiceProvider"
            ],
            "aliases": {
                "AIFactory": "Dgtlss\\Synapse\\Facades\\AIFactory"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}